[sub_resource type="GDScript" id="GDScript_g3xrt"]
resource_name = "Tips"
script/source = "extends PanelContainer

var 当前Tips:CardEntity

func _process(delta: float) -> void:
	pass

func 更新当前Tps(cardData:CardEntity):
	当前Tips=cardData
	if 当前Tips:
		$VBoxContainer/TextureRect.texture=load(当前Tips.get_插画路径())
		if 当前Tips is BaseMinion:
			$\"VBoxContainer/种族\".show()
			$\"VBoxContainer/种族\".text=\"%s\"%[当前Tips.获取种族str()]
		else:
			$\"VBoxContainer/种族\".hide()
		var 描述=当前Tips.获取描述()
		if 描述:
			$\"VBoxContainer/描述\".text=描述
		var 属性加成=\"\"
		for i:AttributeBonus in 当前Tips.永久属性:
			属性加成+=\"%s:%s/%s(永久)\\n\"%[i.加成卡片名称,i.atk_hp.x,i.atk_hp.y]
		for i:AttributeBonus in 当前Tips.临时属性:
			属性加成+=\"%s:%s/%s(临时)\\n\"%[i.加成卡片名称,i.atk_hp.x,i.atk_hp.y]
		if StrUtils.is_blank(属性加成):
			$\"VBoxContainer/属性加成\".hide()
		else:
			$\"VBoxContainer/属性加成\".show()
			$\"VBoxContainer/属性加成\".text=属性加成
	
"

