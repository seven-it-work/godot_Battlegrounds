script/source = "extends BaseMinion

func 攻击其他随从(防御者: BaseMinion):
	await super.攻击其他随从(防御者)
	await _触发最右边战吼()

func _触发最右边战吼():
	if 卡片所在位置 != Enums.CardPosition.战场:
		return
	var 战场随从 = player.获取战场上的牌()
	if 战场随从.is_empty():
		return
	var 最右边随从 = 战场随从[-1] # Get the rightmost minion
	if 最右边随从 is BaseMinion and 最右边随从.战吼.size() > 0:
		var times: int = 1 * 获取倍率() # 1 for normal, 2 for golden
		for i in times:
			for roar in 最右边随从.战吼:
				roar._具体战吼方法接口()
"
