script/source = "extends Deathrattle

func _具体亡语方法接口(攻击者:CardEntity):
	for i in 触发卡.获取倍率():
		var 战场上的牌=触发卡.player.获取战场上的牌().filter(func(item): return item!=触发卡)
		if 战场上的牌.is_empty():
			return
		var card=战场上的牌.pick_random() as BaseMinion
		card.属性加成(AttributeBonus.build(
			触发卡.名称,
			Vector2i(0,触发卡.获取带加成属性().y),
			触发卡.名称,
		), !触发卡.player.是否在战斗中())
	pass
"

